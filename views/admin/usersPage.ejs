<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main class="user__main">
            <h1><%= pageTitle %></h1>
            <% if (vendor.length > 0) { %>
                <article>
                    <% for (let user of vendor) { %>
                        <div class="user__div">
                            <p class="user__email">Email: <%= user.email %></p>
                            <div class="card__actions">
                                <a href="/delete-user/<%= user._id %>" class="delete-btn btn"> Delete <%= pageTitle %></a>
                            </div>
                        </div>
                    <% } %>
                </article>
            <% } else { %>
            <h2>No Vendor found!</h2>
            <% } %>
           
           
        </main>
        <%- include('../includes/end.ejs') %>